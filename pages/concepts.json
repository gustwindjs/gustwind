{
  "meta": {
    "title": "Concepts",
    "description": "Understand Gustwind's concepts on this page"
  },
  "dataSources": [
    {
      "id": "concepts",
      "operation": "file",
      "input": "./documentation/concepts.md",
      "transformWith": "markdown"
    }
  ],
  "page": [
    {
      "component": "mainNavigation"
    },
    {
      "element": "aside",
      "class": "fixed top-16 pl-4 hidden lg:inline",
      "children": [
        {
          "component": "tableOfContents"
        }
      ]
    },
    {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl prose lg:prose-xl",
      "__bind": "concepts",
      "__children": "content",
      "attributes": {
        "id": "main"
      }
    },
    {
      "component": "mainFooter"
    }
  ]
}
