              "children": [
                {
                  "element": "h1",
                  "class": "text-4xl md:text-8xl",
                  "children": [
                    {
                      "element": "span",
                      "class": "whitespace-nowrap pr-4",
                      "children": "üê≥üí®",
                      "_level": 4
                    },
                    {
                      "element": "span",
                      "children": "Gustwind",
                      "_level": 4
                    }
                  ],
                  "_level": 3
                },
                {
                  "element": "h2",
                  "class": "text-xl md:text-4xl font-extralight",
                  "children": "Deno powered JSON oriented site generator",
