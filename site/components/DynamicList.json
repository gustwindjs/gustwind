{
  "type": "ul",
  "class": "flex flex-col gap-2",
  "attributes": {
    "__x-each": "props.dataSource"
  },
  "children": [
    {
      "type": "li",
      "attributes": {
        "x-template": ""
      },
      "class": "flex gap-2",
      "children": [
        {
          "type": "label",
          "attributes": {
            "x": "state.value[0]"
          }
        },
        {
          "type": "input",
          "class": "border flex-grow",
          "attributes": {
            "x-attr": "",
            "type": "text",
            "x": "state.value[1]",
            "@data-field": "state.value[0]",
            "__oninput": "props.oninput"
          }
        }
      ]
    }
  ]
}
