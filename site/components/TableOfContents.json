{
  "type": "nav",
  "attributes": {
    "x-label": "parent",
    "x-state": "{ closest: {}, headings: Array.from(document.querySelectorAll('main h2, h3')) }",
    "x-closest": "{ state: { closest: document.querySelectorAll('main h2, h3') } }"
  },
  "children": [
    {
      "type": "ul",
      "attributes": {
        "x-each": "state.headings"
      },
      "children": [
        {
          "type": "li",
          "attributes": {
            "x-template": ""
          },
          "children": [
            {
              "type": "a",
              "attributes": {
                "x-attr": "",
                "x": "state.value.textContent",
                "@href": "state.value.parentElement?.attributes.href.value",
                "==@class": "`[state.value.textContent === parent.closest.textContent && '` + utilities.tw('underline') + `', state.tagName === 'H3' && '` + utilities.tw('ml-2') + `']`"
              }
            }
          ]
        }
      ]
    }
  ]
}
