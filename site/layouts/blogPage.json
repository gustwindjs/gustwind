{
  "element": "BaseLayout",
  "props": {
    "content": {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl prose lg:prose-xl",
      "children": [
        {
          "element": "h1",
          "__children": "context.match.data.title"
        },
        {
          "element": "p",
          "==children": "utilities.markdown(context.match.content).content"
        }
      ]
    }
  }
}
