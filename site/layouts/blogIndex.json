{
  "type": "BaseLayout",
  "props": {
    "content": {
      "type": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl prose lg:prose-xl",
      "children": [
        {
          "type": "ul",
          "foreach": ["context.blogPosts", {
            "type": "li",
            "class": "inline",
            "children": [
              {
                "type": "Link",
                "props": {
                  "__children": "props.data.title",
                  "==href": "props.data.slug + '/'"
                }
              }
            ]
          }]
        }
      ]
    }
  }
}
