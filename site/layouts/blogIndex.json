{
  "type": "BaseLayout",
  "props": {
    "content": {
      "type": "div",
      "class": "md:mx-auto px-4 md:px-0 w-full lg:max-w-3xl prose lg:prose-xl",
      "children": [
        {
          "type": "ul",
          "foreach": [{
            "utility": "get",
            "parameters": ["context", "blogPosts"]
          }, {
            "type": "li",
            "class": "inline",
            "children": [
              {
                "type": "Link",
                "bindToProps": {
                  "children": {
                    "utility": "get",
                    "parameters": ["props", "data.title"]
                  },
                  "href": {
                    "utility": "concat",
                    "parameters": [
                      {
                        "utility": "get",
                        "parameters": ["props", "data.slug"]
                      },
                      "/"
                    ]
                  }
                }
              }
            ]
          }]
        }
      ]
    }
  }
}
