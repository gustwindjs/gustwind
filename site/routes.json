{
  "/": {
    "layout": "siteIndex",
    "meta": {
      "title": "Gustwind",
      "description": "Deno powered JSON oriented site generator"
    },
    "dataSources": ["readme"],
    "expand": {
      "matchBy": {
        "dataSource": "documentation",
        "slug": "data.slug"
      },
      "layout": "documentationPage",
      "meta": {
        "title": {
          "utility": "get",
          "parameters": ["match", "data.title"]
        },
        "description": {
          "utility": "get",
          "parameters": ["match", "data.description"]
        }
      }
    }
  },
  "blog": {
    "layout": "blogIndex",
    "meta": {
      "title": "Blog",
      "description": "A blog about Gustwind"
    },
    "dataSources": ["blogPosts"],
    "expand": {
      "matchBy": {
        "dataSource": "blogPosts",
        "slug": "data.slug"
      },
      "layout": "blogPage",
      "meta": {
        "title": {
          "utility": "get",
          "parameters": ["match", "data.title"]
        },
        "description": {
          "utility": "get",
          "parameters": ["match", "data.description"]
        }
      }
    }
  },
  "buttons": {
    "layout": "buttonsPage",
    "meta": {
      "title": "Gustwind buttons",
      "description": "Secret buttons page"
    },
    "scripts": ["hello"],
    "routes": {
      "more": {
        "layout": "buttonsPage",
        "meta": {
          "title": "More Gustwind buttons",
          "description": "Another secret buttons page"
        },
        "scripts": ["hello"]
      }
    }
  },
  "playground": {
    "layout": "playgroundPage",
    "meta": {
      "title": "JSON playground",
      "description": "Try JSON to HTML conversion on this page"
    },
    "scripts": ["playground"]
  },
  "atom.xml": {
    "layout": "rssPage",
    "meta": {
      "title": "Gustwind",
      "description": "Gustwind blog"
    },
    "dataSources": ["blogPosts"],
    "type": "xml"
  }
}
