{
  "tasks": {
    "test": "deno test",
    "watch:test": "deno test --watch",
    "start": "deno run --allow-env --allow-read --allow-net --allow-write --allow-run --unstable ./gustwind-cli/mod.ts -d -p 3000",
    "watch:start": "deno run --allow-env --allow-read --allow-net --allow-write --allow-run --unstable --watch ./gustwind-cli/mod.ts -d -p 3000",
    "build": "deno run --allow-env --allow-read --allow-net --allow-write --allow-run --unstable ./gustwind-cli/mod.ts -b -t cpuHalf -o ./build",
    "watch:build": "deno run --allow-env --allow-read --allow-net --allow-write --allow-run --unstable --watch ./gustwind-cli/mod.ts -b -t cpuHalf -o ./build",
    "build:breezewind-for-npm": "deno run -A build_breeze_for_npm.ts",
    "build:gustwind-for-npm": "deno run -A build_gustwind_for_npm.ts"
  }
}
